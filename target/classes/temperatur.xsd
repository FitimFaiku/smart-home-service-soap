<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.wetter-service.com/java"
           targetNamespace="http://www.wetter-service.com/java" elementFormDefault="qualified">

    <xs:element name="getTemperaturRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="city" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getTemperaturResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="temperatur" type="xs:double"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>